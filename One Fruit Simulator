if not game.Players.LocalPlayer:FindFirstChild("bomb") then
_G.IS1 = false
_G.IS2 = false
_G.IS3 = false
_G.IS4 = false
_G.IS5 = false
_G.tpwalk = false
_G.tpwalkSpeed = 0
local startBomb = Instance.new("BoolValue", game.Players.LocalPlayer)
local startDefence = Instance.new("BoolValue", game.Players.LocalPlayer)
startDefence.Value = false
startDefence.Name = "autoTrain"
startBomb.Value = false
startBomb.Name = "bomb"
local bomb = game.Players.LocalPlayer.bomb
local defence = game.Players.LocalPlayer.autoTrain
local lagReduce = false
local BlekLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/laderite/bleklib/main/library.lua"))()
local StartUp = BlekLib:Create({
    Name = "Sticky Hub",
    StartupSound = {
        Toggle = false,
        SoundID = "rbxassetid://6958727243",
        TimePosition = 1
    }
})


local maintab = StartUp:Tab('Main')
local Rapid = StartUp:Tab('Rapid Attack')
local charactertab = StartUp:Tab('Character')
local uitab = StartUp:Tab('Settings')

charactertab:Slider('Speed', 24, 24, 500, function(a)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = a
end)

charactertab:Slider('Jump', 50, 50, 500, function(a)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = a
end)

charactertab:Toggle('TpWalk', function()
    _G.tpwalk = not _G.tpwalk
end)

charactertab:Slider('TpWalk Speed', 0, 0, 10, function(a)
    _G.tpwalkSpeed = a
end)
loadstring(game:HttpGet('https://raw.githubusercontent.com/TempXa/Tpwalk/main/Tpwalk'))()

uitab:Button('Destroy GUI', function()
    StartUp:Exit()
    bomb:Destroy()
    defence:Destroy()
end)

maintab:Toggle('Self Nuke', function(v)
    bomb.Value = v
end)

maintab:Toggle('Auto Defence', function(v)
    defence.Value = v
end)

Rapid:Toggle('Starter Island', function(v)
    IS1 = not IS1
end)

Rapid:Toggle('Monke Island', function(v)
    IS2 = not IS2
end)

Rapid:Toggle('Orange Town', function(v)
    IS3 = not IS3
end)

Rapid:Toggle('Marine Fort', function(v)
    IS4 = not IS4
end)

Rapid:Toggle('Mansion', function(v)
    IS5 = not IS5
    end)
end
--Self Nuke
local bomb = game.Players.LocalPlayer.bomb

bomb:GetPropertyChangedSignal("Value"):Connect(function()
local weapon = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Name
while bomb.Value do wait(0.5)
        print("Checkpoint")
local args = {
    [1] = {
        [1] = {
            [1] = "\3",
            [2] = "skillsControl",
            [3] = weapon,
            [4] = "Z",
            [5] = "Release",
            [6] = nil --[[Vector3]]
        }
    }
}
game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
local args = {
    [1] = {
        [1] = {
            [1] = "\3",
            [2] = "skillsControl",
            [3] = weapon,
            [4] = "X",
            [5] = "Release",
            [6] = nil --[[Vector3]]
        }
    }
}
game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
local args = {
    [1] = {
        [1] = {
            [1] = "\3",
            [2] = "skillsControl",
            [3] = weapon,
            [4] = "V",
            [5] = "Release",
            [6] = nil --[[Vector3]]
        }
    }
}
game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
local args = {
    [1] = {
        [1] = {
            [1] = "\3",
            [2] = "skillsControl",
            [3] = weapon,
            [4] = "B",
            [5] = "Release",
            [6] = nil --[[Vector3]]
        }
    }
}

game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))

        end
end)

--Lag Reduce
workspace.Effects.ChildAdded:Connect(function(child)
    child:Destroy()
end)

--Auto Defence
game.Players.LocalPlayer.autoTrain:GetPropertyChangedSignal("Value"):Connect(function()
if game.Players.LocalPlayer.Backpack:FindFirstChild("Defence") then
game.Players.LocalPlayer.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack.Defence)
game.Players.LocalPlayer.autoTrain.Value = false
game.Players.LocalPlayer.autoTrain.Value = true
else
while game.Players.LocalPlayer.autoTrain.Value do wait()
local args = {
    [1] = {
        [1] = {
            [1] = "\3",
            [2] = "Defence",
            [3] = game:GetService("Players").LocalPlayer.Character.Defence,
            [4] = "Defence"
        }
    }
}

game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
        end
    end
end)
--Rapid Attack

loadstring(game:HttpGet("https://raw.githubusercontent.com/TempXa/Rapid-Attack/main/Islands"))()
